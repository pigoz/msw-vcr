{
  "name": "msw-vcr",
  "version": "0.0.1",
  "description": "msw wrapper to record and replay network requests",
  "scripts": {
    "build": "tsup --clean",
    "circular": "skott --fileExtensions=.ts,.tsx --tsconfig=tsconfig.json --displayMode=raw --showCircularDependencies",
    "circular:viz": "skott --fileExtensions=.ts,.tsx --tsconfig=tsconfig.json",
    "test": "vitest",
    "tc": "tsc --noEmit",
    "version": "changeset version && pnpm install --no-frozen-lockfile",
    "release": "pnpm run build && changeset publish"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.1",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.3.1",
    "@vitest/coverage-v8": "^0.32.2",
    "skott": "^0.24.0",
    "tsup": "^7.1.0",
    "typescript": "^5.1.3",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.32.2"
  },
  "dependencies": {
    "fs-extra": "^11.1.1",
    "msw": "^1.2.2"
  },
  "exports": {
    ".": {
      "require": "./build/cjs/index.js"
    },
    "./*": {
      "require": "./build/cjs/*.js"
    }
  },
  "config": {
    "side": [],
    "modules": [],
    "global": []
  }
}
